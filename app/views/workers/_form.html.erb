<%= form_with model: @worker do |form| %>
    <div>
      <%= form.label :name %><br>
      <%= form.text_field :name %>
     
    </div>
    
    <div>
      <%= form.label :age %><br>
      <%= form.text_field :age %><br>
    </div>

    <div>
        <%= form.label :date_of_birth%>
        <%= form.date_field :date %><br>
    </div>
  
    <div>
      <%= form.submit %>
    </div>
<% end %>